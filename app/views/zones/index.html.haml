        = image_tag "http://ak4.picdn.net/shutterstock/videos/711148/preview/stock-footage-nice-sunset-on-tropical-island.jpg", data: {video: "http://ak4.picdn.net/shutterstock/videos/711148/preview/stock-footage-nice-sunset-on-tropical-island.mp4"}
      %li.jcarousel-item-6
        = image_tag "http://ak2.picdn.net/shutterstock/videos/2992009/preview/stock-footage-tropical-fish-clownfish.jpg", data: {video: "http://ak2.picdn.net/shutterstock/videos/2992009/preview/stock-footage-tropical-fish-clownfish.mp4"}
