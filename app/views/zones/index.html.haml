  .jcarousel-clip
    %ul.carousel.jcarousel-list
      %li.jcarousel-item-1
        = image_tag "http://ak8.picdn.net/shutterstock/videos/2765480/preview/stock-footage--k-timelapse-clouds-over-the-green-field-full-hd-x.jpg", data: {video: "http://ak9.picdn.net/shutterstock/videos/756892/preview/stock-footage-landscape-blue-sky-timelapse.mp4"}
      %li.jcarousel-item-2
        = image_tag "http://ak4.picdn.net/shutterstock/videos/754483/preview/stock-footage-loopable-space-background-with-spinning-camera-cgi-fps.jpg", data: {video: "http://ak4.picdn.net/shutterstock/videos/754483/preview/stock-footage-loopable-space-background-with-spinning-camera-cgi-fps.mp4"}
      %li.jcarousel-item-3
        = image_tag "http://ak6.picdn.net/shutterstock/videos/663274/preview/stock-footage-banyue-or-ban-gioc-waterfall-along-vietnamese-and-chinese-board.jpg", data: {video: "http://ak8.picdn.net/shutterstock/videos/663274/preview/stock-footage-banyue-or-ban-gioc-waterfall-along-vietnamese-and-chinese-board.mp4"}
      %li.jcarousel-item-4
        = image_tag "http://ak2.picdn.net/shutterstock/videos/1920142/preview/stock-footage-sky-lantern-festival.jpg", data: {video: "http://ak2.picdn.net/shutterstock/videos/1920142/preview/stock-footage-sky-lantern-festival.mp4"}
